%users ALL = (%users) NOPASSWD: /usr/share/stool/stool
%stool ALL = (%stool) NOPASSWD: /usr/share/stool/stool
%users ALL = (%users) NOPASSWD:SETENV: /usr/bin/stool-service-wrapper.sh
%stool ALL = (%stool) NOPASSWD:SETENV: /usr/bin/stool-service-wrapper.sh
%users ALL = (root) NOPASSWD: /usr/bin/stool-chowntree.sh
%stool ALL = (root) NOPASSWD: /usr/bin/stool-chowntree.sh
