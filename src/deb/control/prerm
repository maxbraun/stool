#!/bin/sh

set -e

case "$1" in
    remove)
        # may fail if setup did not complete properly
        service stool stop || echo service shutdown failed
        rm -rf /opt/ui/opt/tools/stool
    ;;
    upgrade|deconfigure|failed-upgrade)
    ;;
    *)
        echo "prerm called with unknown argument: $1"
    ;;
esac
exit 0
